<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:gravity="center"
    android:layout_height="wrap_content">
    <LinearLayout
        android:layout_width="@dimen/distance_90"
        android:layout_height="@dimen/distance_30"
        android:layout_marginTop="@dimen/distance_11"
        android:layout_marginBottom="@dimen/distance_5"
        android:background="@drawable/et_background5"
        android:gravity="center"
        tools:ignore="UselessParent">
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        tools:ignore="UseCompoundDrawables,UselessParent">
            <TextView
                android:id="@+id/tv_latest_hot_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#454545"
                android:text="急速28"
                android:textSize="@dimen/text_size_3" />
    </LinearLayout>
    </LinearLayout>
</LinearLayout>