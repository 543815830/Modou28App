<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_marginTop="@dimen/distance_25"
    android:layout_height="wrap_content">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="@dimen/distance_100"
    android:layout_marginLeft="@dimen/distance_15"
    android:layout_marginRight="@dimen/distance_15"
    android:layout_marginBottom="@dimen/distance_5"
   android:orientation="vertical"
    android:gravity="center"
    android:background="@drawable/et_background6"
    tools:ignore="UselessParent">


    <RelativeLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/distance_20"
        android:layout_marginBottom="@dimen/distance_15"
        android:layout_marginRight="@dimen/distance_20">
        <TextView
            android:id="@+id/register_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:textColor="#454545"
            android:textSize="@dimen/text_size_5"
            android:text="@string/_2016_10_10"
            tools:ignore="RelativeOverlap,RtlHardcoded" />

        <TextView
            android:id="@+id/tv_offline_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:textColor="#454545"
            android:textSize="@dimen/text_size_5"
            android:text="@string/_offline_id"
            tools:ignore="RtlHardcoded" />
        <TextView
            android:id="@+id/offline_id"
            android:layout_toRightOf="@+id/tv_offline_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/StatusFColor"
            android:textSize="@dimen/text_size_5"
            tools:ignore="RtlHardcoded" />
    </RelativeLayout>
    <RelativeLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/distance_20"
        android:layout_marginRight="@dimen/distance_20"
        android:layout_marginBottom="@dimen/distance_15">
        <TextView
            android:id="@+id/last_login_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:textColor="#454545"
            android:textSize="@dimen/text_size_5"
            android:text="@string/_2016_10"
            tools:ignore="RelativeOverlap,RtlHardcoded" />

        <TextView
            android:id="@+id/tv_earnings_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:textColor="#454545"
            android:textSize="@dimen/text_size_5"
            android:text="@string/_type"
            tools:ignore="RtlHardcoded" />
        <TextView
            android:id="@+id/earnings_type"
            android:layout_toRightOf="@+id/tv_earnings_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/StatusFColor"
            android:textSize="@dimen/text_size_5"
            tools:ignore="RtlHardcoded" />
    </RelativeLayout>
    <RelativeLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/distance_20"
        android:layout_marginRight="@dimen/distance_20">
        <TextView
            android:id="@+id/tv_award"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:textColor="#454545"
            android:textSize="@dimen/text_size_5"
            android:text="@string/_get_awarze"
            tools:ignore="RtlHardcoded" />
        <TextView
            android:id="@+id/award"
            android:layout_toRightOf="@+id/tv_award"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/StatusFColor"
            android:textSize="@dimen/text_size_5"
            tools:ignore="RtlHardcoded" />
    </RelativeLayout>
</LinearLayout>
</LinearLayout>